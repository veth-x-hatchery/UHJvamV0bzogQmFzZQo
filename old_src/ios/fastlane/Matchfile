type("development") # The default type, can be: appstore, adhoc, enterprise or development

storage_mode("git") 

git_url(ENV["GIT_URL"])

git_basic_authorization(ENV["GIT_BASIC_AUTH"])

app_identifier(ENV["APP_IDENTIFIER"])

